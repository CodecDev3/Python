g = "global var"

def first():
    print("First function")


def g():
    a = "Inside g"
    global b        #"global" will allow b to be used globally (but to make b global, you need to call this function once)
    b = "use me anywhere"
g()
print (b)


def greet(name,name2):
    print("Hello " + name)
    print("Hello Mr.2 " + name2)
    
greet("Dev " + "Patel", "DP")

n = "Prasu"
greet(n,"2")


